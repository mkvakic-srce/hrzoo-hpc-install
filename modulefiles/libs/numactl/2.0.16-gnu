#%Module

set app_ver numactl/v2.0.16-gnu

module-whatis   "Sets up $app_ver in your enviornment"

prepend-path PATH /apps/libs/$app_ver/bin
prepend-path CPATH /apps/libs/$app_ver/include
prepend-path LD_LIBRARY_PATH /apps/libs/$app_ver/lib/
prepend-path PKG_CONFIG_PATH /apps/libs/$app_ver/lib/pkgconfig
prepend-path MANPATH /apps/libs/$app_ver/share/man
